{
    "name": "prototxt",
    "displayName": "prototxt",
    "description": "prototxt syntax highlighting, useful for Caffe users",
    "icon": "images/icon.png",
    "version": "0.0.2",
    "publisher": "dmitry-korobchenko",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "prototxt",
                "aliases": [
                    "prototxt",
                    "prototxt"
                ],
                "extensions": [
                    ".prototxt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "prototxt",
                "scopeName": "source.prototxt",
                "path": "./syntaxes/prototxt.tmLanguage"
            }
        ]
    }
}